{
  "name": "@brreg/designsystemet-spire-theme",
  "version": "0.0.3",
  "description": "Designsystem-tema for Brønnøysundregistrene(spire)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/brreg/designsystemet-spire.git"
  },
  "files": [
    "css"
  ],
  "exports": {
    "./css/*": "./css/*",
    ".": "./css/brreg.css"
  },
  "publishConfig": {
    "access": "public"
  },
  "moduleResolution": "bundler",
  "scripts": {
    "clean": "rimraf css",
    "test": "echo \"Error: no tests specified\" && exit 1",
    "build": "designsystemet tokens create && designsystemet tokens build --clean -o ./css/generated"
  },
  "devDependencies": {
    "@digdir/designsystemet": "^1.7.3",
    "rimraf": "6.1.2"
  },
  "peerDependencies": {
    "@digdir/designsystemet-theme": "^1.0.0"
  }
}
